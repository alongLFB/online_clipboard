{
  "name": "online_clipboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx @cloudflare/next-on-pages --dev",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static --project-name=online-clipboard",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build && wrangler pages deploy .next --project-name=online-clipboard",
    "preview": "npm run build && wrangler pages dev .next --port 3001"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "nanoid": "^5.1.5",
    "next": "15.4.5",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.13",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.28.0"
  }
}
